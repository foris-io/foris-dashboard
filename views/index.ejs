//                    }).
//                    then(function(response)
//                    {
//                        console.log("Got sensor info");
//                        $scope.sensordata = response.data;
//
//                    });
                }

            });

            $scope.logoutsession= function ()
            {
                $http({
                    method: 'GET',
                    url: '/logoutsession'
                }).
                then(function(response)
                {
                    if(response.data.status===300)
                    {
                        console.log("destroy session");
                    }else
                    {
                        $window.location.href='/';
                    }
                });
            }
        }]);

    </script>

